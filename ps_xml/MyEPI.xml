<?xml version="1.0" encoding="utf-8"?>
<Parameters FOVx="200" FOVy="200" GradMaxAmpl="5" GradSlewRate="20" Name="P" Nx="64"
            Ny="64"
            TE="50"
            TR="100">
   <ConcatSequence Name="C1" Repetitions="1">
      <ATOMICSEQUENCE Name="A1">
         <HARDRFPULSE Duration="1" FlipAngle="90" Name="P1" Symmetry="0.5"/>
      </ATOMICSEQUENCE>
      <ATOMICSEQUENCE Name="A2">
         <TRAPGRADPULSE Area="-abs(A)/2" Axis="GX" Name="P2" Observe="A=P4.Area"/>
         <TRAPGRADPULSE Area="-A" Axis="GY" Name="P3" Observe="A=P.KMAXy"/>
      </ATOMICSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="TE" DelayType="C2C" Name="D1" Observe="TE=P.TE" StartSeq="A1"
                           StopSeq="C2"/>
      <CONCATSEQUENCE Name="C2" Observe="N=P.Ny" Repetitions="N">
         <ATOMICSEQUENCE Name="A3">
            <TRAPGRADPULSE ADCs="N" Axis="GX" FlatTopArea="2*A*(-1)^C" FlatTopTime="1" Name="P4"
                           Observe="A=P.KMAXx, C=C2.Counter,N=P.Nx"/>
         </ATOMICSEQUENCE>
         <ATOMICSEQUENCE Name="A4">
            <TRAPGRADPULSE Area="A" Axis="GY" Name="P5" Observe="A=P.DKy"/>
         </ATOMICSEQUENCE>
      </CONCATSEQUENCE>
      <DELAYATOMICSEQUENCE Delay="TR" DelayType="B2E" Name="D2" Observe="TR=P.TR" StartSeq="A1"/>
   </ConcatSequence>
</Parameters>